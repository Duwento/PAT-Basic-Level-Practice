#include<stdio.h>
#include<stdlib.h>
#include<ctype.h>
int main(int argc,char **argv)
{
	int cnt,ins;
	scanf("%d",&cnt);
	int ans[105]={0};
	for(ins=0;ins<cnt;ins++)
	{
		char alpha,is='F';
		while(is!='T')
			scanf("%*c%c-%c",&alpha,&is);
		ans[ins]=alpha-'A'+1;
	}
	for(cnt=0;cnt<ins;cnt++)
		putchar('0'+ans[cnt]);
	putchar('\n');
	return EXIT_SUCCESS;
 } 
